*.aar
*.jar
*.dex
*.class

# 生成的文件
bin/
gen/
out/

# 本地配置
local.properties
gradle.properties
keystore.properties

# 日志文件
*.log
logs/

# Android Studio / IDEA
*.iml
.idea/
.gradle/
.navigation/
captures/
output.json

# 混淆文件
proguard/
/proguard_logs/
mapping.txt
seeds.txt
unused.txt

